    "viewsWelcome": [
      {
        "view": "computor.welcome",
        "contents": "Welcome to Computor!\n\n[Sign In](command:computor.signIn)\n\nSign in to access lecturer features and manage your courses."
      }
    ],
    "views": {
      "computor-lecturer": [
        {
          "id": "computor.lecturerView",
          "name": "Course Management",
          "icon": "$(mortar-board)",
          "contextualTitle": "Lecturer Course Management",
          "when": "computor.authenticated"
        },
        {
          "id": "computor.examplesView",
          "name": "Example Workbench",
          "icon": "$(repo)",
          "contextualTitle": "Example Repository Workbench",
          "when": "computor.authenticated"
        },
        {
          "id": "computor.fileExplorerLecturer",
          "name": "File Explorer",
          "icon": "$(files)",
          "contextualTitle": "File Explorer",
          "when": "computor.authenticated"
        }
      ],
      "computor-student": [
        {
          "id": "computor.studentView",
          "name": "Course Content",
          "icon": "$(book)",
          "contextualTitle": "Student Course Content",
          "when": "computor.authenticated"
        },
        {
          "id": "computor.fileExplorerStudent",
          "name": "Assignment Files",
          "icon": "$(files)",
          "contextualTitle": "Assignment Files",
          "when": "computor.authenticated"
        }
      ],
      "computor-tutor": [
        {
          "id": "computor.tutorView",
          "name": "Student Submissions",
          "icon": "$(checklist)",
          "contextualTitle": "Tutor Review",
          "when": "computor.authenticated"
        },
        {
          "id": "computor.fileExplorerTutor",
          "name": "Submission Files",
          "icon": "$(files)",
          "contextualTitle": "Submission Files",
          "when": "computor.authenticated"
        }
      ],
      "explorer": [
        {
          "id": "computor.apiTreeView",
          "name": "Computor API",
          "icon": "$(globe)",
          "contextualTitle": "Computor API Data",
          "when": "computor.authenticated"
        },
        {
          "id": "computor.jsonTreeView",
          "name": "JSON Explorer",
          "icon": "$(json)",
          "contextualTitle": "JSON Data Explorer"
        },
        {
          "id": "computor.testResultsView",
          "name": "Test Results",
          "icon": "$(beaker)",
          "contextualTitle": "Test Results Explorer"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "computor.refreshExamples",
          "when": "view == computor.examplesView",
          "group": "navigation@1"
        },
        {
          "command": "computor.scanWorkspace",
          "when": "view == computor.examplesView",
          "group": "navigation@3"
        },
        {
          "command": "computor.refreshApiTree",
          "when": "view == computor.apiTreeView",
          "group": "navigation@1"
        },
        {
          "command": "computor.openJsonInEditor",
          "when": "view == computor.jsonTreeView",
          "group": "navigation@1"
        },
        {
          "command": "computor.searchJson",
          "when": "view == computor.jsonTreeView",
          "group": "navigation@2"
        },
        {
          "command": "computor.expandAllJson",
          "when": "view == computor.jsonTreeView",
          "group": "navigation@3"
        },
        {
          "command": "computor.collapseAllJson",
          "when": "view == computor.jsonTreeView",
          "group": "navigation@4"
        },
        {
          "command": "computor.groupTestsBy",
          "when": "view == computor.testResultsView",
          "group": "navigation@1"
        },
        {
          "command": "computor.toggleShowOnlyFailures",
          "when": "view == computor.testResultsView",
          "group": "navigation@2"
        },
        {
          "command": "computor.showTestSummary",
          "when": "view == computor.testResultsView",
          "group": "navigation@3"
        },
        {
          "command": "computor.exportTestResults",
          "when": "view == computor.testResultsView",
          "group": "navigation@4"
        },
        {
          "command": "computor.refreshLecturerTree",
          "when": "view == computor.lecturerView",
          "group": "navigation@1"
        },
        {
          "command": "computor.student.refresh",
          "when": "view == computor.studentView",
          "group": "navigation@1"
        },
        {
          "command": "computor.student.startWorkSession",
          "when": "view == computor.studentView",
          "group": "navigation@1"
        },
        {
          "command": "computor.student.selectCourse",
          "when": "view == computor.studentView",
          "group": "navigation@2"
        },
        {
          "command": "computor.student.refreshCourseContent",
          "when": "view == computor.studentView",
          "group": "navigation@3"
        },
        {
          "command": "computor.student.selectCourse",
          "when": "view == computor.studentView",
          "group": "navigation@2"
        },
        {
          "command": "computor.tutor.refresh",
          "when": "view == computor.tutorView",
          "group": "navigation@1"
        },
        {
          "command": "computor.selectWorkspaceDirectory",
          "when": "view == computor.lecturerView",
          "group": "navigation@2"
        },
        {
          "command": "computor.releaseCourseContent",
          "when": "view == computor.lecturerView",
          "group": "navigation@3"
        },
        {
          "command": "computor.refreshExamples",
          "when": "view == computor.examplesView",
          "group": "navigation@1"
        },
        {
          "command": "computor.searchExamples",
          "when": "view == computor.examplesView",
          "group": "navigation@2"
        },
        {
          "command": "computor.filterExamplesByCategory",
          "when": "view == computor.examplesView",
          "group": "navigation@3"
        },
        {
          "command": "computor.filterExamplesByTags",
          "when": "view == computor.examplesView",
          "group": "navigation@4"
        }
      ],
      "view/item/context": [
        {
          "command": "computor.createNewExample",
          "when": "view == computor.examplesView && viewItem == repository",
          "group": "1_actions@1"
        },
        {
          "command": "computor.uploadExamplesFromZip",
          "when": "view == computor.examplesView && viewItem == repository",
          "group": "1_actions@2"
        },
        {
          "command": "computor.copyApiItemData",
          "when": "view == computor.apiTreeView && viewItem =~ /apiTreeItem/",
          "group": "1_copy@1"
        },
        {
          "command": "computor.openApiDataInEditor",
          "when": "view == computor.apiTreeView && viewItem =~ /apiTreeItem/",
          "group": "1_copy@2"
        },
        {
          "command": "computor.copyJsonValue",
          "when": "view == computor.jsonTreeView && viewItem =~ /jsonTreeItem.*\\.copyable/",
          "group": "1_copy@1"
        },
        {
          "command": "computor.copyJsonPath",
          "when": "view == computor.jsonTreeView && viewItem =~ /jsonTreeItem/",
          "group": "1_copy@2"
        },
        {
          "command": "computor.openJsonInEditor",
          "when": "view == computor.jsonTreeView && viewItem =~ /jsonTreeItem/",
          "group": "2_open@1"
        },
        {
          "command": "computor.goToTest",
          "when": "view == computor.testResultsView && viewItem =~ /testResult/",
          "group": "1_test@1"
        },
        {
          "command": "computor.runSingleTest",
          "when": "view == computor.testResultsView && viewItem =~ /testResult/",
          "group": "1_test@2"
        },
        {
          "command": "computor.createCourseContent",
          "when": "view == computor.lecturerView && viewItem =~ /course\\.contents/",
          "group": "1_content@1"
        },
        {
          "command": "computor.createCourseContent",
          "when": "view == computor.lecturerView && viewItem =~ /courseContent\\.nonSubmittable/",
          "group": "1_content@1"
        },
        {
          "command": "computor.createCourseContentType",
          "when": "view == computor.lecturerView && viewItem == course.contentTypes",
          "group": "1_content@1"
        },
        {
          "command": "computor.createCourseGroup",
          "when": "view == computor.lecturerView && viewItem == course.groups",
          "group": "1_content@1"
        },
        {
          "command": "computor.renameCourseContent",
          "when": "view == computor.lecturerView && viewItem =~ /courseContent/",
          "group": "2_edit@1"
        },
        {
          "command": "computor.deleteCourseContent",
          "when": "view == computor.lecturerView && viewItem =~ /courseContent/",
          "group": "3_delete@1"
        },
        {
          "command": "computor.assignExample",
          "when": "view == computor.lecturerView && viewItem =~ /courseContent.*assignment.*noExample/",
          "group": "4_example@1"
        },
        {
          "command": "computor.unassignExample",
          "when": "view == computor.lecturerView && viewItem =~ /courseContent.*assignment.*hasExample/",
          "group": "4_example@1"
        },
        {
          "command": "computor.showCourseDetails",
          "when": "view == computor.lecturerView && viewItem == course",
          "group": "5_details@1"
        },
        {
          "command": "computor.showCourseContentDetails",
          "when": "view == computor.lecturerView && viewItem =~ /courseContent/",
          "group": "5_details@1"
        },
        {
          "command": "computor.showOrganizationDetails",
          "when": "view == computor.lecturerView && viewItem == organization",
          "group": "5_details@1"
        },
        {
          "command": "computor.showCourseFamilyDetails",
          "when": "view == computor.lecturerView && viewItem == courseFamily",
          "group": "5_details@1"
        },
        {
          "command": "computor.showCourseContentTypeDetails",
          "when": "view == computor.lecturerView && viewItem == courseContentType",
          "group": "5_details@1"
        },
        {
          "command": "computor.showCourseGroupDetails",
          "when": "view == computor.lecturerView && viewItem == course.group",
          "group": "5_details@1"
        },
        {
          "command": "computor.openGitLabRepo",
          "when": "view == computor.lecturerView && viewItem =~ /course/",
          "group": "5_gitlab@1"
        },
        {
          "command": "computor.releaseCourseContent",
          "when": "view == computor.lecturerView && viewItem == course",
          "group": "5_gitlab@2"
        },
        {
          "command": "computor.student.viewCourse",
          "when": "view == computor.studentView && viewItem == studentCourse",
          "group": "1_details@1"
        },
        {
          "command": "computor.student.cloneCourseRepository",
          "when": "view == computor.studentView && viewItem == studentCourse",
          "group": "2_actions@1"
        },
        {
          "command": "computor.student.openInBrowser",
          "when": "view == computor.studentView && viewItem == studentCourse",
          "group": "2_actions@2"
        },
        {
          "command": "computor.student.viewContent",
          "when": "view == computor.studentView && viewItem =~ /studentContent/",
          "group": "1_details@1"
        },
        {
          "command": "computor.student.startAssignment",
          "when": "view == computor.studentView && viewItem == studentContentWithExample",
          "group": "2_actions@1"
        },
        {
          "command": "computor.student.submitAssignment",
          "when": "view == computor.studentView && viewItem == studentContentWithExample",
          "group": "2_actions@2"
        },
        {
          "command": "computor.student.cloneSubmissionGroupRepository",
          "when": "view == computor.studentView && viewItem == studentSubmissionGroupWithRepo",
          "group": "2_actions@1"
        },
        {
          "command": "computor.student.viewSubmissionGroup",
          "when": "view == computor.studentView && viewItem =~ /studentSubmissionGroup/",
          "group": "1_details@1"
        },
        {
          "command": "computor.student.syncAllRepositories",
          "when": "view == computor.studentView && viewItem == studentCourse",
          "group": "3_sync@1"
        },
        {
          "command": "computor.tutor.viewCourse",
          "when": "view == computor.tutorView && viewItem == tutorCourse",
          "group": "1_details@1"
        },
        {
          "command": "computor.tutor.setupWorkspace",
          "when": "view == computor.tutorView && viewItem == tutorCourse",
          "group": "2_actions@1"
        },
        {
          "command": "computor.tutor.viewRepository",
          "when": "view == computor.tutorView && viewItem == tutorExampleRepository",
          "group": "1_details@1"
        },
        {
          "command": "computor.tutor.downloadAllExamples",
          "when": "view == computor.tutorView && viewItem == tutorExampleRepository",
          "group": "2_actions@1"
        },
        {
          "command": "computor.tutor.viewExample",
          "when": "view == computor.tutorView && viewItem == tutorExample",
          "group": "1_details@1"
        },
        {
          "command": "computor.tutor.downloadExample",
          "when": "view == computor.tutorView && viewItem == tutorExample",
          "group": "2_actions@1"
        },
        {
          "command": "computor.tutor.downloadExampleWithDependencies",
          "when": "view == computor.tutorView && viewItem == tutorExample",
          "group": "2_actions@2"
        },
        {
          "command": "computor.tutor.openExampleInBrowser",
          "when": "view == computor.tutorView && viewItem == tutorExample",
          "group": "3_external@1"
        },
        {
          "command": "computor.checkoutExample",
          "when": "view == computor.examplesView && viewItem == example",
          "group": "1_actions@1"
        },
        {
          "command": "computor.uploadExample",
          "when": "view == computor.examplesView && viewItem == example",
          "group": "1_actions@2"
        },
        {
          "command": "computor.scanWorkspace",
          "when": "view == computor.examplesView",
          "group": "2_workspace@1"
        }
      ]
    }
  },
