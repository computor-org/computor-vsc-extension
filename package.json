          "group": "navigation@2"
        },
        {
          "command": "computor.releaseCourseContent",
          "when": "view == computor.lecturer.courses",
          "group": "navigation@3"
        },
        {
          "command": "computor.refreshExamples",
          "when": "view == computor.lecturer.examples",
          "group": "navigation@1"
        },
        {
          "command": "computor.searchExamples",
          "when": "view == computor.lecturer.examples",
          "group": "navigation@2"
        },
        {
          "command": "computor.filterExamplesByCategory",
          "when": "view == computor.lecturer.examples",
          "group": "navigation@3"
        },
        {
          "command": "computor.filterExamplesByTags",
          "when": "view == computor.lecturer.examples",
          "group": "navigation@4"
        },
        {
          "command": "computor.selectWorkspaceDirectory",
          "when": "view == computor.fileExplorerLecturer",
          "group": "navigation@1"
        },
        {
          "command": "computor.fileExplorer.refresh",
          "when": "view == computor.fileExplorerLecturer",
          "group": "navigation@2"
        },
        {
          "command": "computor.fileExplorer.refresh",
          "when": "view == computor.fileExplorerStudent",
          "group": "navigation@2"
        },
        {
          "command": "computor.selectWorkspaceDirectory",
          "when": "view == computor.fileExplorerTutor",
          "group": "navigation@1"
        },
        {
          "command": "computor.fileExplorer.refresh",
          "when": "view == computor.fileExplorerTutor",
          "group": "navigation@2"
        }
      ],
      "view/item/context": [
        {
          "command": "computor.createNewExample",
          "when": "view == computor.lecturer.examples && viewItem == exampleRepository",
          "group": "1_actions@1"
        },
        {
          "command": "computor.uploadExamplesFromZip",
          "when": "view == computor.lecturer.examples && viewItem == exampleRepository",
          "group": "1_actions@2"
        },
        {
          "command": "computor.copyJsonValue",
          "when": "view == computor.jsonTreeView && viewItem =~ /jsonTreeItem.*\\.copyable/",
          "group": "1_copy@1"
        },
        {
          "command": "computor.copyJsonPath",
          "when": "view == computor.jsonTreeView && viewItem =~ /jsonTreeItem/",
          "group": "1_copy@2"
        },
        {
          "command": "computor.openJsonInEditor",
          "when": "view == computor.jsonTreeView && viewItem =~ /jsonTreeItem/",
          "group": "2_open@1"
        },
        {
          "command": "computor.goToTest",
          "when": "view == computor.testResultsView && viewItem =~ /testResult/",
          "group": "1_test@1"
        },
        {
          "command": "computor.runSingleTest",
          "when": "view == computor.testResultsView && viewItem =~ /testResult/",
          "group": "1_test@2"
        },
        {
          "command": "computor.createCourseContent",
          "when": "view == computor.lecturer.courses && viewItem =~ /course\\.contents/",
          "group": "1_content@1"
        },
        {
          "command": "computor.createCourseContent",
          "when": "view == computor.lecturer.courses && viewItem =~ /courseContent\\.nonSubmittable/",
          "group": "1_content@1"
        },
        {
          "command": "computor.createCourseContentType",
          "when": "view == computor.lecturer.courses && viewItem == course.contentTypes",
          "group": "1_content@1"
        },
        {
          "command": "computor.createCourseGroup",
          "when": "view == computor.lecturer.courses && viewItem == course.groups",
          "group": "1_content@1"
        },
        {
          "command": "computor.renameCourseContent",
          "when": "view == computor.lecturer.courses && viewItem =~ /courseContent/",
          "group": "2_edit@1"
        },
        {
          "command": "computor.deleteCourseContent",
          "when": "view == computor.lecturer.courses && viewItem =~ /courseContent/",
          "group": "3_delete@1"
        },
        {
          "command": "computor.assignExample",
          "when": "view == computor.lecturer.courses && viewItem =~ /courseContent.*assignment.*noExample/",
          "group": "4_example@1"
        },
        {
          "command": "computor.unassignExample",
          "when": "view == computor.lecturer.courses && viewItem =~ /courseContent.*assignment.*hasExample/",
          "group": "4_example@1"
        },
        {
          "command": "computor.showCourseDetails",
          "when": "view == computor.lecturer.courses && viewItem == course",
          "group": "5_details@1"
        },
        {
          "command": "computor.showCourseContentDetails",
          "when": "view == computor.lecturer.courses && viewItem =~ /courseContent/",
          "group": "5_details@1"
        },
        {
          "command": "computor.showOrganizationDetails",
          "when": "view == computor.lecturer.courses && viewItem == organization",
          "group": "5_details@1"
        },
        {
          "command": "computor.showCourseFamilyDetails",
          "when": "view == computor.lecturer.courses && viewItem == courseFamily",
          "group": "5_details@1"
        },
        {
          "command": "computor.showCourseContentTypeDetails",
          "when": "view == computor.lecturer.courses && viewItem == courseContentType",
          "group": "5_details@1"
        },
        {
          "command": "computor.showCourseGroupDetails",
          "when": "view == computor.lecturer.courses && viewItem == course.group",
          "group": "5_details@1"
        },
        {
          "command": "computor.openGitLabRepo",
          "when": "view == computor.lecturer.courses && viewItem =~ /course/",
          "group": "5_gitlab@1"
        },
        {
          "command": "computor.releaseCourseContent",
          "when": "view == computor.lecturer.courses && viewItem == course",
          "group": "5_gitlab@2"
        },
        {
          "command": "computor.student.viewCourse",
          "when": "view == computor.student.courses && viewItem == studentCourse",
          "group": "1_details@1"
        },
        {
          "command": "computor.student.openInBrowser",
          "when": "view == computor.student.courses && viewItem == studentCourse",
          "group": "2_actions@2"
        },
        {
          "command": "computor.student.viewContent",
          "when": "view == computor.student.courses && viewItem =~ /studentCourseContent/",
          "group": "1_details@1"
        },
        {
          "command": "computor.student.showPreview",
          "when": "view == computor.student.courses && viewItem =~ /studentCourseContent.*assignment/",
          "group": "1_details@2"
        },
        {
          "command": "computor.student.showPreview",
          "when": "view == computor.student.courses && viewItem =~ /studentCourseContent.*assignment/",
          "group": "inline"
        },
        {
          "command": "computor.student.startAssignment",
          "when": "view == computor.student.courses && viewItem == studentContentWithExample",
          "group": "2_actions@1"
        },
        {
          "command": "computor.student.submitAssignment",
          "when": "view == computor.student.courses && viewItem == studentContentWithExample",
          "group": "2_actions@2"
        },
        {
          "command": "computor.student.cloneSubmissionGroupRepository",
          "when": "view == computor.student.courses && viewItem == studentSubmissionGroupWithRepo",
          "group": "2_actions@1"
        },
        {
          "command": "computor.student.viewSubmissionGroup",
          "when": "view == computor.student.courses && viewItem =~ /studentSubmissionGroup/",
          "group": "1_details@1"
        },
        {
          "command": "computor.tutor.viewCourse",
          "when": "view == computor.tutor.courses && viewItem == tutorCourse",
          "group": "1_details@1"
        },
        {
          "command": "computor.tutor.setupWorkspace",
          "when": "view == computor.tutor.courses && viewItem == tutorCourse",
          "group": "2_actions@1"
        },
        {
          "command": "computor.tutor.viewRepository",
          "when": "view == computor.tutor.courses && viewItem == tutorExampleRepository",
          "group": "1_details@1"
        },
        {
          "command": "computor.tutor.downloadAllExamples",
          "when": "view == computor.tutor.courses && viewItem == tutorExampleRepository",
          "group": "2_actions@1"
        },
        {
          "command": "computor.tutor.viewExample",
          "when": "view == computor.tutor.courses && viewItem == tutorExample",
          "group": "1_details@1"
        },
        {
          "command": "computor.tutor.downloadExample",
          "when": "view == computor.tutor.courses && viewItem == tutorExample",
          "group": "2_actions@1"
        },
        {
          "command": "computor.tutor.downloadExampleWithDependencies",
          "when": "view == computor.tutor.courses && viewItem == tutorExample",
          "group": "2_actions@2"
        },
        {
          "command": "computor.tutor.openExampleInBrowser",
          "when": "view == computor.tutor.courses && viewItem == tutorExample",
          "group": "3_external@1"
        },
        {
          "command": "computor.checkoutExample",
          "when": "view == computor.lecturer.examples && viewItem == example",
          "group": "1_actions@1"
        },
        {
          "command": "computor.uploadExample",
          "when": "view == computor.lecturer.examples && viewItem == example",
          "group": "1_actions@2"
        },
        {
          "command": "computor.scanWorkspace",
          "when": "view == computor.lecturer.examples",
          "group": "2_workspace@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "mocha",
    "test:unit": "mocha --config .mocharc.unit.json",
