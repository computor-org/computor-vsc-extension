{
  "name": "computor-vsc-extension",
  "displayName": "Computor VS Code Extension",
  "description": "VS Code extension for computor teaching software",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:computor.activate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "computor.activate",
        "title": "Activate Computor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4",
    "@types/mocha": "^10.0.1",
    "@types/chai": "^4.3.4",
    "mocha": "^10.1.0",
    "chai": "^4.3.7"
  },
  "dependencies": {
    "node-fetch": "^3.3.0"
  }
}