        {
          "id": "computor.testResultsView",
          "name": "Test Results",
          "icon": "$(beaker)",
          "contextualTitle": "Test Results Explorer"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "computor.refreshExamples",
          "when": "view == computor.lecturer.examples",
          "group": "navigation@1"
        },
        {
          "command": "computor.scanWorkspace",
          "when": "view == computor.lecturer.examples",
          "group": "navigation@3"
        },
        {
          "command": "computor.openJsonInEditor",
          "when": "view == computor.jsonTreeView",
