    "viewsContainers": {
      "activitybar": [
        {
          "id": "computor-lecturer",
          "title": "Lecturer",
          "icon": "$(mortar-board)",
          "when": "ctutor.lecturer.treeview.show"
        },
        {
          "id": "computor-student",
          "title": "Student",
          "icon": "$(book)",
          "when": "ctutor.student.treeview.show"
        },
        {
          "id": "computor-tutor",
          "title": "Tutor",
          "icon": "$(person)",
          "when": "ctutor.lecturer.treeview.show"
        }
      ]
    },
    "views": {
      "computor-lecturer": [
        {
          "id": "computor.lecturer.courses",
          "name": "Courses",
          "icon": "$(mortar-board)",
          "contextualTitle": "Lecturer Courses"
        },
        {
          "id": "computor.lecturer.examples",
          "name": "Examples",
          "icon": "$(repo)",
          "contextualTitle": "Example Repository"
