{
  "name": "computor-webview-dev",
  "version": "0.1.0",
  "private": true,
  "description": "Local development playground for Computor webview UI with mocked data.",
  "scripts": {
    "start": "node server.js",
    "view:comments": "nodemon server.js --view comments --open",
    "view:messages": "nodemon server.js --view messages --open",
    "view:settings": "nodemon server.js --view settings --open",
    "view:showcase": "nodemon server.js --view showcase --open",
